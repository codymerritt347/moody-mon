<% if Helpers.is_logged_in?(session) %>
  <h1>Your Entries</h1>

  <ol>
  <% @entries.each do |entry| %>
    <li><a href="entries/<%= entry.id %>">
    <%= entry.time_of_day %>
    <%= entry.feeling %>
    <%= entry.intensity %>
    <%= entry.situation %>
    </a></li>
  <% end %>
  </ol>

  <a href="/users/<%= @user.id %>">Your Homepage</a>
<% else %>
  <h1>You are not logged in!</h1>
  <a href="/sessions/login">Log In</a>
<% end %>